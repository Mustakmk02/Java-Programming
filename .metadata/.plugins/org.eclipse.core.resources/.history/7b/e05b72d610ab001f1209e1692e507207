
public class SwappingTwoNumber
{
	public static void main(String[] args) 
	{
		int a=10,b=20;
				
		  System.out.println("Before Swapping Values are : "+a+" "+b);
		  
		  //Logic #1
		  int tamp=a;
		         a=b;
		         b=tamp;
		   System.out.println("After Swapping Values are : "+a+" "+b);
		  
		
	}

}
