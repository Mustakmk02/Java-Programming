
public class ReverseString 
{
	public static void main(String[] args)
	{
         String str="MUSTAK";
         String rev="";
         int len=str.length();
         for(int i=str.length()-1;i>=0;i--)
         {
        	 rev =rev+str.charAt(3);
         }
         System.out.println(rev);
		
	}

}
